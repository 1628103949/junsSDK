<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.smwl.smsdk.myview.mall.MallTurntableLevelRewardView
        android:id="@+id/first_guaranteed_rewards_item"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        app:is_land="true"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/first_divide_line"
        android:layout_width="7dp"
        android:layout_height="23dp"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toBottomOf="@id/second_guaranteed_rewards_item"
        app:layout_constraintStart_toStartOf="parent" />

    <com.smwl.smsdk.myview.mall.MallTurntableLevelRewardView
        android:id="@+id/second_guaranteed_rewards_item"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginTop="7dp"
        android:visibility="gone"
        app:is_land="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/first_guaranteed_rewards_item" />

    <View
        android:id="@+id/second_divide_line"
        android:layout_width="7dp"
        android:layout_height="23dp"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toBottomOf="@id/third_guaranteed_rewards_item"
        app:layout_constraintStart_toStartOf="parent" />

    <com.smwl.smsdk.myview.mall.MallTurntableLevelRewardView
        android:id="@+id/third_guaranteed_rewards_item"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginTop="7dp"
        android:visibility="gone"
        app:is_land="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/second_guaranteed_rewards_item" />

    <View
        android:id="@+id/third_divide_line"
        android:layout_width="7dp"
        android:layout_height="23dp"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toBottomOf="@id/fourth_guaranteed_rewards_item"
        app:layout_constraintStart_toStartOf="parent" />

    <com.smwl.smsdk.myview.mall.MallTurntableLevelRewardView
        android:id="@+id/fourth_guaranteed_rewards_item"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginTop="7dp"
        android:visibility="gone"
        app:is_land="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/third_guaranteed_rewards_item" />

    <View
        android:id="@+id/fourth_divide_line"
        android:layout_width="7dp"
        android:layout_height="23dp"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toBottomOf="@id/fifth_guaranteed_rewards_item"
        app:layout_constraintStart_toStartOf="parent" />

    <com.smwl.smsdk.myview.mall.MallTurntableLevelRewardView
        android:id="@+id/fifth_guaranteed_rewards_item"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginTop="7dp"
        android:visibility="gone"
        app:is_land="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fourth_guaranteed_rewards_item" />

</android.support.constraint.ConstraintLayout>