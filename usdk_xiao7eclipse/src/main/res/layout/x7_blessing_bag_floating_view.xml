<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/rl_root_float_ad_left"
        android:layout_width="@dimen/text_size_45"
        android:layout_height="@dimen/text_size_45"
        android:background="@drawable/x7_oval_solid_99000000"
        android:visibility="visible">

        <ImageView
            android:id="@+id/count_down_blessing_bag_tips_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/x7_blessing_bag_tips" />

        <com.smwl.smsdk.myview.floating.ArcProgressView
            android:id="@+id/arc_progress_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </RelativeLayout>

</RelativeLayout>