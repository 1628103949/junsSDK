    <color name="permissionBgColorGreenLight">#44A49C</color>
    <color name="permissionBgColorBlue">#ee447AA4</color>
    <color name="pb_bg_color">#EBEBEB</color>
    <color name="pb_transparent">#00000000</color>
    <color name="pb_line">#CCCCCC</color>
    <color name="pb_gonelue_nav">#FD9027</color>
    <color name="pb_gonelue_nav_pressed">#fea44e</color>
    <color name="pb_gift_state_off">#B8B8B8</color>
    <color name="pb_area_pickerview_timebtn_nor">#057dff</color>
    <color name="pb_area_pickerview_timebtn_pre">#c2daf5</color>
    <color name="pb_area_pickerview_bg_topbar">#f5f5f5</color>
    <color name="pb_area_pickerview_topbar_title">#000000</color>
    <color name="pb_area_pickerview_wheelview_textcolor_out">#a8a8a8</color>
    <color name="pb_area_pickerview_wheelview_textcolor_center">#2a2a2a</color>
    <color name="pb_area_pickerview_wheelview_textcolor_divider">#d5d5d5</color>
