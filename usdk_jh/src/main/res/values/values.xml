    <style name="SDKAnimation">
        <item name="android:windowEnterAnimation">@anim/a_sdk_popup_enter</item>
        <item name="android:windowExitAnimation">@anim/a_sdk_popup_out</item>
    </style>
    <style name="Theme.SDKTransparent" parent="@android:style/Theme.Dialog">
        <!-- 设置允许窗口半透明 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 引用Android系统的动画半透明样式 -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <!-- 设定窗口背景颜色 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 隐藏窗口标题栏 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowCloseOnTouchOutside">@null</item>
        <!--Dialog的windowFrame框为无-->
        <item name="android:windowFrame">@null</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsFloating">true</item>
        <!--背景是否模糊显示-->
        <item name="android:backgroundDimEnabled">true</item>
<!--        <item name="android:windowAnimationStyle">@style/Theme.SDKInOut</item>-->
    </style>
    <style name="YXFcustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsTranslucent">true</item>
        <!-- 隐藏窗口标题栏 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="popupWindowToTop">
        <item name="android:windowEnterAnimation">@anim/a_sdk_popup_enter</item>
        <item name="android:windowExitAnimation">@anim/a_sdk_popup_to_top</item>
    </style>
