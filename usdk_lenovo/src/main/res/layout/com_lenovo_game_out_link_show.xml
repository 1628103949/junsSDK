<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="bottom"
    android:orientation="vertical"
    android:background="@color/com_lenovo_game_white"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:orientation="vertical"
        >
        <ImageView
            android:id="@+id/com_lenovo_game_outlink_iv_back"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_centerVertical="true"
            android:src="@drawable/com_lenovo_game_back"
            />
        <ImageView
            android:id="@+id/com_lenovo_game_outlink_iv_close"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:src="@drawable/com_lenovo_game_close"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            />
    </RelativeLayout>
    <FrameLayout
        android:id="@+id/lenovo_game_fl_load_error"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/corner_half_balck_3_bg"
        >
        <WebView
            android:id="@+id/com_lenovo_game_outlink_wv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="invisible"
            />
        <ProgressBar
            android:id="@+id/com_lenovo_game_outlink_loading"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:indeterminateDrawable="@drawable/com_lenovo_pay_progress_pb_circle"
            android:indeterminate="false"
            android:layout_gravity="center"
            />
    </FrameLayout>
</LinearLayout>
