<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/com_lenovo_lsf_text_ripple_musk">
    <item>
        <selector android:variablePadding="false" android:constantSize="true" >
            <item android:state_enabled="false">
                <shape android:shape="rectangle">
                    <solid android:color="@color/com_lenovo_lsf_text_ripple_musk" />
                    <corners android:radius="2dp" />
                </shape>
            </item>
            <item android:state_enabled="true">
                <shape android:shape="rectangle">
                    <solid android:color="@color/com_lenovo_lsf_next_button" />
                    <corners android:radius="2dp" />
                </shape>
            </item>
        </selector>
    </item>
</selector>
