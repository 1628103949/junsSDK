    <style name="Theme.Dialog.Kiwi" parent="android:Theme.Dialog">

        <!-- Window attributes -->
        <item name="android:windowSoftInputMode">adjustNothing|stateAlwaysHidden</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/hyberry_transparent</item>

        <!-- Widget styles -->
        <item name="android:imageButtonStyle">@style/Widget.ImageButton.Kiwi</item>
        <item name="android:textViewStyle">@style/Widget.TextView.Kiwi</item>
        <item name="android:buttonStyle">@style/Widget.Button.Kiwi</item>
        <item name="android:editTextStyle">@style/Widget.EditText.Kiwi</item>
        <item name="android:progressBarStyle">@style/Widget.ProgressBar.Kiwi</item>

        <!-- Text styles -->
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ff333333</item>

        <!-- Text selection handle attributes -->
        <item name="android:textCursorDrawable">@drawable/hyberry_text_cursor</item>
    </style>
    <style name="Widget.Button.Kiwi" parent="android:Widget.Button">
        <item name="android:background">@drawable/hyberry_state_button_default</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">4dp</item>
        <item name="android:gravity">center</item>
        <item name="android:minWidth">60dp</item>
        <item name="android:minHeight">30dp</item>
    </style>
    <style name="Widget.EditText.Kiwi" parent="android:Widget.EditText">
        <item name="android:background">@drawable/hyberry_state_background_edit</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">4dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:selectAllOnFocus">true</item>
    </style>
    <style name="Widget.ImageButton.Kiwi" parent="android:Widget.ImageButton">
        <item name="android:background">@color/hyberry_transparent</item>
    </style>
    <style name="Widget.ProgressBar.Kiwi" parent="android:Widget">
        <item name="android:indeterminateOnly">true</item>
        <item name="android:indeterminateDrawable">@drawable/hyberry_animation_progress</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:indeterminateDuration">1500</item>
        <item name="android:minWidth">29dip</item>
        <item name="android:maxWidth">29dip</item>
        <item name="android:minHeight">29dip</item>
        <item name="android:maxHeight">29dip</item>
