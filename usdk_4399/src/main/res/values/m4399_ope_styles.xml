<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="m4399.Operate.Orientation.Adaptive" parent="m4399">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="m4399TranslucentFullscreenActivityTheme" parent="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" />

    <style name="m4399.Operate.Theme.Dialog.Fullscreen" parent="android:Theme.Light.NoTitleBar.Fullscreen">
        <item name="android:windowEnterAnimation">@anim/m4399_ope_support_slide_in_right</item>
        <item name="android:windowExitAnimation">@anim/m4399_ope_support_slide_out_right</item>
    </style>

    <style name="m4399.Operate.Theme.Dialog.Translucent" parent="m4399.Theme.Dialog.Base">
        <!--弹窗背景是否变暗-->
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="m4399.Operate.Theme.Dialog.Content.Translucent" parent="m4399.Theme.Dialog.Base">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="m4399.Operate.Anim.UserCenterDialog" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/m4399_ope_support_slide_in_right</item>
        <item name="android:windowExitAnimation">@anim/m4399_ope_support_slide_out_right</item>
    </style>

    <style name="m4399.Operate.UserCenterDialog.Theme" parent="m4399.Operate.Theme.Dialog.Fullscreen">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="m4399.UserCenter.Entry" parent="m4399">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/m4399_ope_support_item_bg</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="m4399.Operate.Anti.NameAuth.Edit" parent="m4399">
        <item name="android:textColorHint">@color/m4399_ope_color_bbbbbb</item>
        <item name="android:textSize">@dimen/m4399_font_14</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:background">@drawable/m4399_ope_shape_r2_outline_gray</item>
        <item name="android:textColor">@color/m4399_ope_color_666666</item>
        <item name="android:textCursorDrawable">@drawable/m4399_ope_shape_edt_cursor</item>
    </style>

    <style name="m4399PerfectLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/m4399_ope_perfect_line</item>
    </style>

    <!-- 自定义对话框样式 -->

    <style name="m4399_sms_verify_editor">
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/m4399_ope_color_666666</item>
        <item name="android:textSize">@dimen/m4399_font_16</item>
        <item name="android:textColorHint">@color/m4399_ope_color_bbbbbb</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:ellipsize">end</item>
    </style>

    <!-- EditText -->
    <style name="m4399_Style_Edit_Base" parent="m4399">
        <item name="android:textColorHint">@color/m4399_ope_color_bbbbbb</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:background">@drawable/m4399_ope_shape_r2_outline_gray</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/m4399_ope_color_666666</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textCursorDrawable">@drawable/m4399_ope_shape_edt_cursor</item>
    </style>

    <style name="m4399.Pay.Page" parent="m4399">
        <item name="android:background">#ffffff</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="m4399.Pay.Body" parent="m4399">
        <item name="android:background">#ffffff</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_marginLeft">@dimen/m4399_activity_spacing_h</item>
        <item name="android:layout_marginTop">@dimen/m4399_pay_spacing_v</item>
        <item name="android:layout_marginRight">@dimen/m4399_activity_spacing_h</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:fillViewport">true</item>
        <item name="android:scrollbars">none</item>
    </style>

    <style name="m4399.Pay.Channel.Container" parent="m4399.Operate.Orientation.Adaptive">
        <item name="android:layout_marginTop">@dimen/m4399_pay_spacing_v_half</item>
    </style>

    <style name="m4399.Pay.Label" parent="m4399">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="m4399.Pay.Label.Value" parent="m4399">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="m4399.Pay.Label.Card" parent="m4399.Pay.Label">
        <item name="android:layout_marginRight">22dp</item>
    </style>

    <style name="m4399.Pay.Card.Editor" parent="m4399">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:background">@drawable/m4399_ope_shape_r2_outline_gray</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#111111</item>
        <item name="android:textColorHint">#c9c9c9</item>
        <item name="android:inputType">number</item>
        <item name="android:imeOptions">actionDone</item>
    </style>

    <style name="m4399.Pay.Handy.Money.Table" parent="m4399">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:numColumns">@integer/m4399_pay_handy_money_table_col</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:verticalSpacing">8dp</item>
        <item name="android:horizontalSpacing">@dimen/m4399_pay_card_money_horizontal_spacing</item>
        <item name="android:columnWidth">@dimen/m4399_pay_card_width</item>
        <item name="android:stretchMode">none</item>
        <item name="android:isScrollContainer">false</item>
        <item name="android:choiceMode">singleChoice</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:scrollbars">none</item>
    </style>

    <style name="m4399AnimBanner">
        <item name="android:windowEnterAnimation">@anim/m4399_ope_banner_enter</item>
        <item name="android:windowExitAnimation">@anim/m4399_ope_banner_exit</item>
    </style>

</resources>