<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="@android:color/white" />
            <corners
                android:bottomLeftRadius="5dp"
                android:topLeftRadius="5dp" />
            <size
                android:width="79dp"
                android:height="65dp" />
        </shape>
    </item>

    <item
        android:bottom="60dp"
        android:left="74dp">
        <shape>
            <size
                android:width="4.5dp"
                android:height="4.5dp" />
            <solid android:color="@color/m4399_ope_coupon_dialog_bg_end" />
            <corners android:bottomLeftRadius="60dp" />
        </shape>
    </item>

    <item
        android:left="60dp"
        android:right="-18dp">
        <rotate android:fromDegrees="90">
            <shape android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="@color/m4399_ope_color_e5e5e5"
                    android:dashWidth="2dp"
                    android:dashGap="2dp" />
            </shape>
        </rotate>
    </item>

    <item
        android:left="74dp"
        android:top="60dp">
        <shape>
            <size
                android:width="4.5dp"
                android:height="4.5dp" />
            <solid android:color="@color/m4399_ope_coupon_dialog_bg_end" />
            <corners android:topLeftRadius="60dp" />
        </shape>
    </item>

</layer-list>